{
  "name": "@pauloluan/list-sample",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "acessibility:start": "lerna run dev --scope @pauloluan/list-sample",
    "bootstrap": "lerna bootstrap && yarn build",
    "build": "lerna run --stream build",
    "bump": "yarn npm-check-updates -u",
    "clean": "lerna clean --yes && rm -rf node_modules",
    "clean:dist": "yarn lerna exec -- rimraf dist",
    "clean:force": "git clean -xfd && yarn",
    "dedup": "yarn-deduplicate --strategy highest yarn.lock",
    "deps:upgrade": "yarn sort-package-json && yarn bump && yarn dedup && yarn",
    "predev": "yarn bootstrap",
    "dev": "lerna run dev --stream --parallel",
    "format": "eslint --fix packages --ext .js,.jsx,.ts,.tsx",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "prestart": "yarn bootstrap",
    "start": "lerna run start --stream --parallel",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "storybook": "concurrently \"yarn storybook:start\"",
    "storybook:start": "start-storybook -p 6006 -s .storybook/public && yarn bootstrap",
    "test": "lerna run --parallel test",
    "test:watch": "lerna run --parallel test:watch"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "core-js": "^3.21.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "styled-components": "^5.3.3",
    "styled-icons": "^10.45.0",
    "styled-media-query": "^2.1.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-storysource": "^6.4.18",
    "@storybook/addons": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.18",
    "@storybook/theming": "^6.4.18",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/jest-axe": "^3.5.3",
    "@types/lodash": "4.14.178",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.22",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vitejs/plugin-react": "^1.2.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-vite-meta-env": "1.0.3",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.6.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "jest-styled-components": "^7.0.8",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.3",
    "npm-check-updates": "^12.3.0",
    "plop": "^3.0.5",
    "prettier": "2.5.1",
    "sass": "1.49.7",
    "sass-loader": "12",
    "sort-package-json": "^1.54.0",
    "style-loader": "^3.3.1",
    "ts-jest": "27.1.3",
    "ts-node": "10.5.0",
    "typescript": "4.5.5",
    "vite": "^2.8.0",
    "vite-plugin-filter-replace": "0.1.9",
    "vite-preset-react": "^2.2.0",
    "yarn-deduplicate": "^3.1.0"
  }
}
